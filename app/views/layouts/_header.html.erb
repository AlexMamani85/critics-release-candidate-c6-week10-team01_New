<header class="header">

<header class="header">
      <div class="container navbar">
        <a href="/games" class="logo">
          <img src="/images/logo.svg" alt="Codeable" height="32" />
        </a>
        <nav>
          <ul class="navbar-links list-style-none" role="list">
            <% if user_signed_in? %>
            <li>Welcome <%= current_user.username %></li>
            <% end %>

            <li><%= link_to 'Companies', companies_path, class: "link" %></li>
            <li><%= link_to 'Games', games_path, class: "link" %></li>

            <% if user_signed_in? %>
            <li><%= link_to "Edit Profile", edit_user_registration_path, class: "link" %></li>
            <li><%= link_to "Logout", destroy_user_session_path, data: { turbo_method: :delete }, class: "link" %></li>
            <% else %>

            <li><%= link_to "Sign in", new_user_session_path, class: "link" %></li>
            <li><%= link_to "Sign up", new_user_registration_path, class: "link" %></li>
            <% end %>
          </ul>
        </nav>


        
        <%=image_tag "icons/menu.svg", class: "navbar__menu-icon"%>
        <%=image_tag "icons/close.svg", class: "navbar__menu-icon-close"%>
      </div>
    </header>



        <hr>
    </br>


</header>
