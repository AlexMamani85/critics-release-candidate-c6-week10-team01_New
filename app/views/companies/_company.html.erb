<div id="<%= dom_id company %>">

  <div>
  <% if company.cover.attached? %>
    <div>
      <%= image_tag(company.cover, class: "category-image") %>
    </div>
  <% end %>
</div>

  <p>
    <%= company.name %>
  </p>

  <p>
    <%= company.start_date %>
    <%= company.country %>
  </p>

  <p>
    <%= company.description %>
  </p>

</div>
